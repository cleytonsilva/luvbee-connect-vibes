# Arquivos que devem ser tratados como texto
*.md text eol=lf
*.json text eol=lf
*.ts text eol=lf
*.tsx text eol=lf
*.js text eol=lf
*.jsx text eol=lf
*.css text eol=lf
*.html text eol=lf
*.sql text eol=lf

# Arquivos bin√°rios
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.svg binary
*.webp binary
*.mp4 binary
*.webm binary
*.woff binary
*.woff2 binary
*.ttf binary
*.eot binary

# Arquivos que nunca devem ser commitados
.env filter=git-crypt diff=git-crypt
.env.local filter=git-crypt diff=git-crypt
*.key filter=git-crypt diff=git-crypt
*.pem filter=git-crypt diff=git-crypt

